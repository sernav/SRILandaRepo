<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="es/uclm/sri/recursos/database.properties" />
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="${database.driver}" />
				<property name="url" value="${database.url}" />
				<property name="username" value="${database.username}" />
				<property name="password" value="${database.password}" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper
			resource="es/uclm/sri/persistencia/postgre/dao/sqlmap/AlbumsappMapper.xml" />
		<mapper
			resource="es/uclm/sri/persistencia/postgre/dao/sqlmap/HistuserMapper.xml" />
		<mapper
			resource="es/uclm/sri/persistencia/postgre/dao/sqlmap/UsersappMapper.xml" />
		<mapper
			resource="es/uclm/sri/persistencia/postgre/dao/sqlmap/VpesosalbumMapper.xml" />
		<mapper
			resource="es/uclm/sri/persistencia/postgre/dao/sqlmap/VpesosuserMapper.xml" />
		<mapper
			resource="es/uclm/sri/persistencia/postgre/dao/sqlmap/PesosalbumMapper.xml" />
	</mappers>
</configuration>